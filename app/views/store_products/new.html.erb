<% if @store_product.product %>
  <h1><%= @store_product.product.name %></h1>
<% elsif @store_product.store %>
  <h1><%= @store_product.store.name %></h1>
<% end %>

<%= simple_form_for @store_product do |f| %>
  <%= f.association :store %>
  <%= f.association :product %>
  <%= f.input :price %>
  <%= f.button :submit %>
<% end %>